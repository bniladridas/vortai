{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/index.py?method=generate"
    },
    {
      "src": "/api/generate-with-thinking",
      "dest": "/api/index.py?method=generate_with_thinking"
    },
    {
      "src": "/api/generate-with-url-context",
      "dest": "/api/index.py?method=generate_with_url_context"
    },
    {
      "src": "/api/text-to-speech",
      "dest": "/api/index.py?method=text_to_speech"
    },
    {
      "src": "/api/generate-image",
      "dest": "/api/index.py?method=generate_image"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
